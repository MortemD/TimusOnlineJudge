import java.util.Scanner;

public class Timus {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        double number;
        double primer;

        for (int i = 0; i < n; i++) {
            number = in.nextInt();
            primer = Math.sqrt(8 * number - 7);

            if (primer % 1 == 0) System.out.println(1);
            else System.out.println(0);
        }
    }
}
