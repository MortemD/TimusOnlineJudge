import java.util.Scanner;
//Подключаем сканер чтобы осуществлять ввод с клавиатуры

public class Doing {
    public static void main(String[] args) {
        //Создаём сканнер для ввода
        Scanner aScanner = new Scanner(System.in);

        //Переменная, которая будет помогать сохранять значения
        //Будет находится в цикле в квадратных скобках массива
        //Так как в цикле while нет инициализации локальной переменной под это
        int usefulTemporary = 0;

        //Создаём массив для хранения чисел
        //Которые мы потом вводим и находим их корень
        //В задании сказано, что "Размер входного потока непревышает 256КБ"
        //Значит размер массива сделаем 256*1000
        //Массив делаем именно double, чтобы сохранять
        double[] number = new double[256*1000];

        //Пока вводится число типа long
        //(от –9 223 372 036 854 775 808 до 9 223 372 036 854 775 807)
        //.hasNextLong() проверяет введено ли число типа long
        while (aScanner.hasNextLong()) {
            //Квадратный корень - это Math.sqrt
            //Сохраняет значения в массиве сначала в 0 позиции, потом в 1 и т.д.
            //Сканер настраиваем на получение очень больших чисел - чисел long
            number[usefulTemporary++] = Math.sqrt(aScanner.nextLong());
        }

        //Осуществляем вывод чисел с помощью цикла for
        //Используем декремент чтобы выводить числа с конца
        //Переменная i равна usefulTemporary - 1, так как элементы в массиве начинаются с 0, а не с единицы
        //Применяем printf для вывода с форматированием
        //printf значит вывод 4 цифр после запятой
        // n нужен для того, чтобы после вывода был перевод на новую строку
        for (int i = usefulTemporary - 1; i >= 0; i --)
        {
            System.out.printf("%.4f", number[i]);
            System.out.print('\n');
        }
    }
}
