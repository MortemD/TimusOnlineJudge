import java.util.*;

public class Timus {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        short firstCode = in.nextShort(); // Should %2 == 0
        short secondCode = in.nextShort(); // Should %2 != 0

        if (firstCode % 2 == 0 || secondCode % 2 !=0) System.out.println("yes");
        else System.out.println("no");
    }
}
