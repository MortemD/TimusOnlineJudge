import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int f = in.nextInt();
        int last = 12 - f;

        if ((last * 45) / 60 >= 4) System.out.println("NO");
        else System.out.println("YES");
    }
}
