import java.util.*;

public class Timus {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        int n = in.nextInt();
        int m = in.nextInt();
        System.out.println(n*(m+1)); //plus second for counting 0 example m = 4: 0 1 2 3 4 * count of times n
    }
}
